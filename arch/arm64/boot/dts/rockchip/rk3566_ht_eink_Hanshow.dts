&i2c1 {
    status = "okay";
    clock-frequency = <400000>;

#ifdef TYPEC_fusb30x
    usbc0: fusb30x@22 {
        status = "okay";
        compatible = "fairchild,fusb302";
        reg = <0x22>;
        pinctrl-names = "default";
        pinctrl-0 = <&fusb0_int>;
        //pinctrl-0 = <&fusb0_int>, <&fusb0_switch>;
        //power-supply = <&vccio_sd>;
        //vbus-5v-gpios = <&gpio1 4 GPIO_ACTIVE_LOW>;
        int-n-gpios = <&gpio0 RK_PC6 IRQ_TYPE_LEVEL_LOW>;
        //switch-gpios = <&gpio1 RK_PD4 GPIO_ACTIVE_HIGH>;
    };
#else
    usbc0: et7303@4e {
        status = "okay";
        compatible = "etek,et7303";
        reg = <0x4e>;
        interrupt-parent = <&gpio0>;
        interrupts = <22 IRQ_TYPE_LEVEL_LOW>;
        pinctrl-names = "default";
        pinctrl-0 = <&et7303_int>;

        usb_con: connector {
            data-role = "dual";
            power-role = "dual";
            try-power-role = "sink";
            sink-pdos = <PDO_FIXED(5000, 2000, PDO_FIXED_USB_COMM)>;
            source-pdos = <PDO_FIXED(5000, 2000, PDO_FIXED_USB_COMM)>;
            op-sink-microwatt = <10000000>;
        };
    };
