        goodix,resume-in-workqueue = <0>;
        goodix,int-sync = <1>;
        goodix,swap-x2y = <1>;
        goodix,x2x = <1>;
        goodix,y2y = <1>;
        goodix,esd-protect = <1>;
        goodix,auto-update-cfg = <0>;
        goodix,power-off-sleep = <0>;
        goodix,pen-suppress-finger = <0>;
        goodix,cfg-group0 = [
            47 50 07 7C 05 0A 05 00 01 0F 28 05 5F 46 03 05 00 00 00 00 00 00 08 00 00 00 00 8E 2E AB 65 67 D9 0B 00 00 01 B9 02 1C 00 00 00 00 00 03 00 00 00 00 00 46 78 94 C0 02 07 00 00 04 91 49 00 89 52 00 84 5B 00 7F 66 00 7D 71 00 7D 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 01 04 05 06 07 08 09 0C 0D 0E 0F 10 11 14 15 16 17 18 19 1A FF FF FF FF FF FF FF FF FF 00 02 04 06 07 08 0A 0C 0D 0F 10 11 12 13 19 1B 1C 1E 1F 20 21 22 23 24 25 26 27 28 FF FF FF FF FF FF FF FF FF FF FF FF FF FF 7E 01
        ];
    };
