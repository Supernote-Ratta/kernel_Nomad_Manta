&pinctrl {
    pmic {
        pmic_int: pmic_int {
            rockchip,pins =
                <0 RK_PA3 RK_FUNC_GPIO &pcfg_pull_up>;
        };

        soc_slppin_gpio: soc_slppin_gpio {
            rockchip,pins =
                <0 RK_PA2 RK_FUNC_GPIO &pcfg_output_low_pull_down>;
        };

        soc_slppin_slp: soc_slppin_slp {
            rockchip,pins =
                <0 RK_PA2 1 &pcfg_pull_up>;
        };

        soc_slppin_rst: soc_slppin_rst {
            rockchip,pins =
                <0 RK_PA2 2 &pcfg_pull_none>;
        };
    };

    eink_pmic {
        einkpmic_gpio: einkpmic-gpio {
            rockchip,pins =
                <3 RK_PA7 RK_FUNC_GPIO &pcfg_pull_none>,
                <3 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>,
                <1 RK_PB1 RK_FUNC_GPIO &pcfg_pull_down>,// dummy EBC_PWR_EN
                <3 RK_PB1 RK_FUNC_GPIO &pcfg_pull_down>,// SPK_CTL
                <3 RK_PB4 RK_FUNC_GPIO &pcfg_pull_none>,
                <4 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
        };
    };

    sdio-pwrseq {
        wifi_enable_h: wifi-enable-h {
            rockchip,pins = <0 RK_PC3 RK_FUNC_GPIO &pcfg_pull_none>;// WIFI_REG_ON_H
        };
    };

    wireless-wlan {
        wifi_gpio: wifi-gpio {
            rockchip,pins =
                <0 RK_PD3 RK_FUNC_GPIO &pcfg_pull_up>,// WIFI_WAKE_HOST_H
                <0 RK_PC7 RK_FUNC_GPIO &pcfg_pull_none>;// WIFI_PWR_EN
        };
    };

    wireless-bluetooth {
        uart1_gpios: uart1-gpios {
            rockchip,pins = <2 RK_PB5 RK_FUNC_GPIO &pcfg_pull_none>;
        };

        bt_reset_gpio: bt-reset-gpio {
            rockchip,pins = <0 RK_PC4 RK_FUNC_GPIO &pcfg_pull_none>;
        };

        bt_wake_gpio: bt-wake-gpio {
            rockchip,pins = <0 RK_PD4 RK_FUNC_GPIO &pcfg_pull_none>;
        };

        bt_irq_gpio: bt-irq-gpio {
            rockchip,pins = <0 RK_PD5 RK_FUNC_GPIO &pcfg_pull_up>;// BT_WAKE_HOST_H
        };
    };

    mxc6655xa {
        mxc6655xa_irq_gpio: mxc6655xa-irq-gpio {
            rockchip,pins = <0 RK_PD6 RK_FUNC_GPIO &pcfg_pull_none>;
        };
    };

    sensors {
        hall_irq_gpio: hall-irq-gpio {
            rockchip,pins = <0 RK_PA0 RK_FUNC_GPIO  &pcfg_pull_up>;
        };
    };

    typec {
        et7303_int: et7303-int {
            rockchip,pins = <0 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>;
        };
        fusb0_int: fusb0-int {
            rockchip,pins = <0 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>;
        };
    };

    wacom {
        emr_gpio: emr-gpio {
            rockchip,pins =
                <0 RK_PB5 RK_FUNC_GPIO &pcfg_pull_up>,// EMR_PEN_DETECT
                <0 RK_PB6 RK_FUNC_GPIO &pcfg_pull_up>,
                <0 RK_PB0 RK_FUNC_GPIO &pcfg_pull_up>;
        };
    };

    goodix {
        tp_gpio: tp-gpio {
            rockchip,pins =
                <0 RK_PB5 RK_FUNC_GPIO &pcfg_pull_up>,
                <0 RK_PC1 RK_FUNC_GPIO &pcfg_pull_up>;
        };
    };

   tsc {
        tsc_gpio: tsc-gpio {
            rockchip,pins =
                <0 RK_PA6 RK_FUNC_GPIO &pcfg_pull_up>,// TP_RST_L
                <0 RK_PC1 RK_FUNC_GPIO &pcfg_pull_up>;
        };
    };
};

